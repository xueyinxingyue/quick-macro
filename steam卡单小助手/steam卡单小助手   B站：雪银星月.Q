Sub gift()
	Do
		FindPic 0,0,1920,1080,"Attachment:\按好友名称搜索.bmp",1,intX,intY
		If intX > 0 And intY > 0 Then 
			MoveTo intX+20, intY+10
			Delay 500
			LeftClick 1
			Delay 1000
			SayString Form1.InputBox1.Text
			KeyPress "Space", 1
			Goto 选择好友
		End If
	Loop
	
	Rem 选择好友
	Do
		Delay 1000
		FindPic 0,0,1920,1080,"Attachment:\选择好友.bmp",1,intX,intY
		If intX > 0 And intY > 0 Then 
			MoveTo intX, intY
			LeftClick 1
		End If
		FindPic 0,0,1920,1080,"Attachment:\继续.bmp",1,intX,intY
		If intX > 0 And intY > 0 Then 
			MoveTo intX, intY
			LeftClick 1
			Delay 500
			Goto 接收人
		End If
	Loop
	
	Rem 接收人
	Do
		FindPic 0,0,1920,1080,"Attachment:\接收人名字.bmp",1,intX,intY
		If intX > 0 And intY > 0 Then 
			MoveTo intX + 20, intY + 30
			LeftClick 1
			Delay 500
			KeyPress "1", 1
			Delay 500
			Goto 签名继续
		End If
	Loop

	Rem 签名继续
	FindPic 0,0,1920,1080,"Attachment:\礼物信息.bmp",1,intX,intY
	If intX > 0 And intY > 0 Then 
		MoveTo intX + 20, intY + 30
		LeftClick 1
		KeyPress "1", 1
	End If
	FindPic 0,0,1920,1080,"Attachment:\您的签名.bmp",1,intX,intY
	If intX > 0 And intY > 0 Then 
		MoveTo intX + 20, intY + 30
		LeftClick 1
		KeyPress "1", 1
	End If
	FindPic 0,0,1920,1080,"Attachment:\继续.bmp",1,intX,intY
	If intX > 0 And intY > 0 Then 
		MoveTo intX, intY
		LeftClick 1
		Delay 300
	End If
	End Sub
	
